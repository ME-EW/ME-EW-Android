<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_sign_up"
    app:startDestination="@id/characterSelectFragment">

    <fragment
        android:id="@+id/characterSelectFragment"
        android:name="com.kangmin.meew.view.signup.CharacterSelectFragment"
        android:label="CharacterSelectFragment"
        tools:layout="@layout/fragment_character_select">
        <action
            android:id="@+id/action_characterSelectFragment_to_nickNameFragment"
            app:destination="@id/nickNameFragment" />
    </fragment>
    <fragment
        android:id="@+id/nickNameFragment"
        android:name="com.kangmin.meew.view.signup.NickNameFragment"
        android:label="NickNameFragment"
        tools:layout="@layout/fragment_nick_name"/>
</navigation>