<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:color="@color/grey_500" android:width="1dp"/>
            <corners android:topLeftRadius="23dp" android:topRightRadius="23dp"/>
            <padding android:bottom="0dp"
                android:top="0dp"
                android:right="0dp"
                android:left="0dp"/>
        </shape>
    </item>

    <item android:top="1dp" android:right="0.5dp" android:left="0.5dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/grey_700"/>
            <corners android:topRightRadius="24dp" android:topLeftRadius="24dp"/>
        </shape>
    </item>


</layer-list>